<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="tela"
        class="br.gov.sp.fatec.Tela">
        <property name="nome" ref="registro"/>
    </bean>
    <bean id="registro"
        class="br.gov.sp.fatec.Dados">
        <property name="nome" value="Paula"/>
        <property name="sobrenome" value=" Nemer"/>
        <property name="Telefone" value=" 123456"/>
        <property name="profissao" value=" Tecnico em Informatica"/>
    </bean>
</beans>